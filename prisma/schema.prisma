generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Order {
  id               String   @id @default(auto()) @map("_id") @db.ObjectId
  symbol           String
  quantity         String
  price            String
  type             String
  availableBalance String?
  executedAt       DateTime @default(now())
}
